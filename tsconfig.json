{
    "compilerOptions": {

        "pretty": true,


        "moduleResolution": "node",
        "declaration": true,
        "noEmitHelpers": true,


        "removeComments": false,
        "preserveConstEnums": true,

        "sourceMap": true,




        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules", "lib"
    ],
    "awesomeTypescriptLoaderOptions": {
        "useBabel": true
    }
}
